<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OrderProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
   <servlet>
  	<servlet-name>f1</servlet-name>
  	<servlet-class>com.csmtech.runner.Controller</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>f1</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>f2</servlet-name>
  	<servlet-class>com.csmtech.runner.OrderDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>f2</servlet-name>
  	<url-pattern>/orderDetails</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>f3</servlet-name>
  	<servlet-class>com.csmtech.runner.ButtonAction</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>f3</servlet-name>
  	<url-pattern>/ordersByName</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>f4</servlet-name>
  	<servlet-class>com.csmtech.runner.OrderDetailsByName</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>f4</servlet-name>
  	<url-pattern>/allproductInTable</url-pattern>
  </servlet-mapping>
  
</web-app>